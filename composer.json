{
  "name": "slcorp/logger-crud-bundle",
  "version": "2.0.1",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "stability": "stable",
  "description": "A clean architecture Role Model bundle for Symfony.",
  "keywords": [
    "symfony",
    "role",
    "bundle",
    "clean architecture"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Zhalayletdinov Vyacheslav",
      "email": "evil_tut@mail.ru"
    }
  ],
  "require": {
    "php": ">=8.4",
    "slcorp/core-bundle": "^2.0.5",
    "symfony/framework-bundle": "^7.1",
    "doctrine/dbal": "^4.3.4",
    "doctrine/orm": "^3.3",
    "doctrine/doctrine-migrations-bundle": "^3.5",
    "symfony/runtime": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/yaml": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/monolog-bundle": "^3.10",
    "symfony/flex": "^2.9",
    "doctrine/doctrine-bundle": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/phpunit-bridge": "^6.0",
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.89",
    "phpunit/phpunit": "^12.4",
    "symfony/maker-bundle": "^1.61",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-phpunit": "^2.0",
    "symfony/browser-kit": "^7.1",
    "symfony/css-selector": "^7.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/stereo-type/bundles-core.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slcorp\\LoggerCrudBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slcorp\\LoggerCrudBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev",
      "stable": "master"
    },
    "symfony": {
      "allow-contrib": true,
      "recipe": "config/recipes"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-fix": "php-cs-fixer fix",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true
    }
  }
}
