{
  "name": "academcity/logger-crud-bundle",
  "version": "1.3.7",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "stability": "stable",
  "description": "A clean architecture Role Model bundle for Symfony.",
  "keywords": [
    "symfony",
    "role",
    "bundle",
    "clean architecture"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Zhalayletdinov Vyacheslav",
      "email": "evil_tut@mail.ru"
    }
  ],
  "require": {
    "php": ">=8.2",
    "academcity/core-bundle": "^1.3.7",
    "symfony/framework-bundle": "^7.1",
    "doctrine/dbal": "^4.2.1",
    "doctrine/orm": "^3.3",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "symfony/runtime": "^7.1",
    "symfony/security-bundle": "^7.1",
    "symfony/security-core": "^7.2",
    "symfony/yaml": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/monolog-bundle": "^3.10",
    "symfony/flex": "^2.4",
    "doctrine/doctrine-bundle": "^2.13"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/phpunit-bridge": "^6.0",
    "phpstan/phpstan": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpunit/phpunit": "^11.4",
    "symfony/maker-bundle": "^1.61",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-phpunit": "^2.0",
    "symfony/browser-kit": "^7.1",
    "symfony/css-selector": "^7.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://oauth2:JRB8vueyHk5JnBfUjNsb@gitlab.dev-u.ru/bundles/core_bundle.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "AcademCity\\LoggerCrudBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AcademCity\\LoggerCrudBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev",
      "stable": "master"
    },
    "symfony": {
      "allow-contrib": true,
      "recipe": "config/recipes"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-fix": "php-cs-fixer fix",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true
    }
  }
}
